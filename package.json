{
  "name": "map-canvas",
  "version": "1.0.0",
  "description": "基于baidu、google地图的canvas扩展",
  "main": "build/typhoon.js",
  "scripts": {
    "mini": "npm run build && uglifyjs build/typhoon.js -c -m -o build/typhoon.min.js",
    "build": "rollup -c",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "src",
        "main.js"
      ],
      "extensions": "js,jsx",
      "ignore": ""
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chengquan223/map-canvas.git"
  },
  "author": "309581454@qq.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chengquan223/map-canvas/issues"
  },
  "homepage": "https://github.com/chengquan223/map-canvas#readme",
  "devDependencies": {
    "babel-preset-es2015-rollup": "^1.2.0",
    "npm-watch": "^0.1.6",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.6.1",
    "uglify-js": "^2.7.4"
  }
}